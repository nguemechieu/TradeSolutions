<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="780.0" prefWidth="1580.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.tradeadviser.tradesolutions.TradeSolutionsController">
    <HBox layoutX="432.0" layoutY="547.0" prefHeight="57.0" prefWidth="99.0">
        <Button fx:id="cancelId" mnemonicParsing="false" text="Cancel"/>
    </HBox>
    <HBox layoutX="521.0" layoutY="547.0" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #273311;"
          styleClass="CreateAccount"/>
    <GridPane layoutX="362.0" layoutY="282.0" prefHeight="236.0" prefWidth="353.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="171.0" minWidth="10.0" prefWidth="87.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="277.0" minWidth="10.0" prefWidth="265.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <children>
            <Label text="Username  :" GridPane.rowIndex="1"/>
            <Label text="Password   :" GridPane.rowIndex="2"/>
            <Label prefHeight="21.0" prefWidth="127.0" text="E-mail        :" GridPane.rowIndex="3"/>
            <TextField fx:id="usernameId" onAction="#getUsername" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <TextField fx:id="emailId" onAction="#getEmail" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            <PasswordField fx:id="passId" onAction="#getPassword" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <Label text="First Name:" GridPane.rowIndex="4"/>
            <Label text="Last Name :" GridPane.rowIndex="5"/>
            <Label text="MiddleName  :" GridPane.rowIndex="6"/>
            <TextField fx:id="firstnameId" onAction="#getFirstname" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
            <TextField fx:id="middleNameId" onAction="#getMiddlenameId" prefHeight="0.0" prefWidth="112.0"
                       GridPane.columnIndex="1" GridPane.rowIndex="6"/>
            <TextField fx:id="lastnameId" onAction="#getLastname" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
            <Label text="USER_ID :#"/>
            <Label fx:id="dLbl" text="1" GridPane.columnIndex="1"/>
        </children>
    </GridPane>
    <VBox layoutX="504.0" layoutY="646.0" prefHeight="40.0" prefWidth="161.0"/>
    <Button fx:id="registerId" layoutX="648.0" layoutY="544.0" mnemonicParsing="false" onAction="#getRegister"
            text="Submit"/>
</AnchorPane>
